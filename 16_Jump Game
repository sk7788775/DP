class Solution {
    public boolean canJump(int[] nums) {
        return solve(nums,nums.length-1);
    }
    
    boolean solve(int[] arr,int ind) {
        if(ind==0) return true;
        if(ind<0) return false;
        for(int i=ind-1;i>=0;i--) {
            int d=ind-i;
            if(arr[i]>=d && solve(arr,i))
               return true;
        }
        return false;
    }
}
